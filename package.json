{
  "name": "no-popup-windows",
  "version": "1.0.0",
  "description": "Web extension to move popup windows into non-popup windows as a new tab",
  "browser": "dist/monitor.js",
  "scripts": {
    "build": "tsc && node build.js",
    "build:builder": "tsc build.ts --target esnext --module esnext --moduleResolution node"
  },
  "type": "module",
  "keywords": [
    "web extension",
    "popup windows",
    "new tab"
  ],
  "author": "Isitea <isitea@isitea.net> (https://isitea.net)",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "^0.0.191",
    "@types/firefox": "^0.0.31",
    "@types/firefox-webext-browser": "^94.0.1",
    "@types/naver-whale": "^0.0.2",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.3",
    "@typescript-eslint/parser": "^5.30.3",
    "eslint": "^8.19.0",
    "regex-copy": "^1.0.3",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@shopify/draggable": "1.0.0-beta.8"
  }
}